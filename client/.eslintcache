ile-forms/AddEducation';\nimport PrivateRoute from './components/routing/PrivateRoute';\nimport Profiles from './components/profiles/Profiles';\nimport Profile from './components/profile/Profile';\nimport Posts from './components/posts/Post';\nimport moduleName from 'module';\n\n// Redux\nimport { Provider } from 'react-redux';\nimport store from './store';\nimport { loadUser } from './actions/auth';\nimport setAuthToken from './utils/setAuthToken';\n\nimport './App.css';\n\nif (localStorage.token) {\n\tsetAuthToken(localStorage.token);\n}\n\nconst App = () => {\n\tuseEffect(() => {\n\t\tstore.dispatch(loadUser());\n\t\t//emty brakets sayesinde sadece bir kere run yapar\n\t}, []);\n\treturn (\n\t\t<Provider store={store}>\n\t\t\t<Router>\n\t\t\t\t<Fragment>\n\t\t\t\t\t<Navbar />\n\t\t\t\t\t<Route exact path='/' component={Landing} />\n\t\t\t\t\t<section className='container'>\n\t\t\t\t\t\t<Alert />\n\t\t\t\t\t\t<Switch>\n\t\t\t\t\t\t\t<Route exact path='/register' component={Register} />\n\t\t\t\t\t\t\t<Route exact path='/login' component={Login} />\n\t\t\t\t\t\t\t<Route exact path='/profiles' component={Profiles} />\n\t\t\t\t\t\t\t<Route exact path='/profile/:id' component={Profile} />\n\t\t\t\t\t\t\t<PrivateRoute exact path='/dashboard' component={Dashboard} />\n\t\t\t\t\t\t\t<PrivateRoute\n\t\t\t\t\t\t\t\texact\n\t\t\t\t\t\t\t\tpath='/create-profile'\n\t\t\t\t\t\t\t\tcomponent={CreateProfile}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<PrivateRoute\n\t\t\t\t\t\t\t\texact\n\t\t\t\t\t\t\t\tpath='/edit-profile'\n\t\t\t\t\t\t\t\tcomponent={EditProfile}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<PrivateRoute\n\t\t\t\t\t\t\t\texact\n\t\t\t\t\t\t\t\tpath='/add-experience'\n\t\t\t\t\t\t\t\tcomponent={AddExperience}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<PrivateRoute\n\t\t\t\t\t\t\t\texact\n\t\t\t\t\t\t\t\tpath='/add-education'\n\t\t\t\t\t\t\t\tcomponent={AddEducation}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<PrivateRoute exact path='/posts' component={Posts} />\n\t\t\t\t\t\t</Switch>\n\t\t\t\t\t</section>\n\t\t\t\t</Fragment>\n\t\t\t</Router>\n\t\t</Provider>\n\t);\n};\n\nexport default App;\n","/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/components/layouts/Landing.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/components/layouts/Navbar.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/components/auth/Login.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/components/auth/Register.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/store.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/reducers/index.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/reducers/alert.js",["168"],"import { SET_ALERT, REMOVE_ALERT } from '../actions/types.js';\n\nconst initialState = [];\n\n// eslint-disable-next-line import/no-anonymous-default-export\nexport default function (state = initialState, action) {\n\tconst { type, payload } = action;\n\n\tswitch (action.type) {\n\t\tcase SET_ALERT:\n\t\t\treturn [...state, payload];\n\n\t\tcase REMOVE_ALERT:\n\t\t\treturn state.filter(alert => alert.id !== payload);\n\n\t\tdefault:\n\t\t\treturn state;\n\t}\n}\n","/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/actions/types.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/actions/alert.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/components/layouts/Alert.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/actions/auth.js",[],"/Users/serdarsalci/Projects/TraversyMedia/MernStack Front to Back/DEVCONNECTOR/client/src/reducers/auth.js",[],"/Users/serdarsalci/Projects/Traversy